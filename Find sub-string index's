      String s1="naveenkumar";
	    String s2="nave";
	    for(int i=0;i<s1.length();i++){
	        if(s1.charAt(i)==s2.charAt(0)){
	            int n=0,count=0;
	            for(int j=i;j<i+s2.length() && j<s1.length();j++,n++){
	                if(s1.charAt(j)==s2.charAt(n)){
	                    count++;
	                }
	            }
	            if(count==s2.length()){
	                System.out.println(i+"  "+(i+n-1));
	            }
	        }
	    }
